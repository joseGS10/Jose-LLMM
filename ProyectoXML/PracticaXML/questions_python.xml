<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE test [
  <!ELEMENT test (question+)>
  <!ELEMENT question (wording,choices)>
  <!ELEMENT wording (#PCDATA)>
  <!ELEMENT choices (choice+)>
  <!ELEMENT choice (#PCDATA)>
  <!ATTLIST question id CDATA #REQUIRED>
  <!ATTLIST choice correct (yes|no) 'no'>
]>
<test>
  <question id="1">
    <wording>What is Python?</wording>
    <choices>
      <choice correct="yes">A high-level programming language</choice>
      <choice>A type of snake</choice>
      <choice>An operating system</choice>
      <choice>A database</choice>
    </choices>
  </question>
  <question id="2">
    <wording>Who created Python?</wording>
    <choices>
      <choice>Linus Torvalds</choice>
      <choice>Dennis Ritchie</choice>
      <choice>James Gosling</choice>
      <choice correct="yes">Guido van Rossum</choice>
    </choices>
  </question>
  <question id="3">
    <wording>What symbol is used for comments in Python?</wording>
    <choices>
      <choice correct="yes">#</choice>
      <choice>//</choice>
      <choice>&amp;lt;!-- --&amp;gt;</choice>
      <choice>/* */</choice>
    </choices>
  </question>
  <question id="4">
    <wording>How do you define a function in Python?</wording>
    <choices>
      <choice>By using the keyword function</choice>
      <choice correct="yes">By using the keyword def</choice>
      <choice>By using func()</choice>
      <choice>By using lambda without def</choice>
    </choices>
  </question>
  <question id="5">
    <wording>What structure is used to repeat instructions?</wording>
    <choices>
      <choice correct="yes">for and while loops</choice>
      <choice>if conditional</choice>
      <choice>class structure</choice>
      <choice>importing modules</choice>
    </choices>
  </question>
  <question id="6">
    <wording>What type of language is Python?</wording>
    <choices>
      <choice>Compiled</choice>
      <choice>Binary</choice>
      <choice correct="yes">Interpreted</choice>
      <choice>Assembly</choice>
    </choices>
  </question>
  <question id="7">
    <wording>What function prints to the screen in Python?</wording>
    <choices>
      <choice correct="yes">print()</choice>
      <choice>echo()</choice>
      <choice>display()</choice>
      <choice>log()</choice>
    </choices>
  </question>
  <question id="8">
    <wording>What data type represents decimal numbers?</wording>
    <choices>
      <choice>int</choice>
      <choice>str</choice>
      <choice>bool</choice>
      <choice correct="yes">float</choice>
    </choices>
  </question>
  <question id="9">
    <wording>What operator is used for integer division?</wording>
    <choices>
      <choice correct="yes">//</choice>
      <choice>/</choice>
      <choice>%</choice>
      <choice>**</choice>
    </choices>
  </question>
  <question id="10">
    <wording>What data structure stores key-value pairs?</wording>
    <choices>
      <choice>list</choice>
      <choice correct="yes">dictionary (dict)</choice>
      <choice>tuple</choice>
      <choice>set</choice>
    </choices>
  </question>
  <question id="11">
    <wording>What keyword is used to handle exceptions?</wording>
    <choices>
      <choice correct="yes">try</choice>
      <choice>catch</choice>
      <choice>handle</choice>
      <choice>rescue</choice>
    </choices>
  </question>
  <question id="12">
    <wording>What method converts a string to lowercase?</wording>
    <choices>
      <choice correct="yes">lower()</choice>
      <choice>min()</choice>
      <choice>downcase()</choice>
      <choice>tolower()</choice>
    </choices>
  </question>
  <question id="13">
    <wording>What operator is used to concatenate strings?</wording>
    <choices>
      <choice>&amp;</choice>
      <choice>*</choice>
      <choice correct="yes">+</choice>
      <choice>#</choice>
    </choices>
  </question>
  <question id="14">
    <wording>What function returns the length of a list?</wording>
    <choices>
      <choice correct="yes">len()</choice>
      <choice>length()</choice>
      <choice>count()</choice>
      <choice>size()</choice>
    </choices>
  </question>
  <question id="15">
    <wording>What keyword is used to define a class?</wording>
    <choices>
      <choice correct="yes">class</choice>
      <choice>def</choice>
      <choice>object</choice>
      <choice>struct</choice>
    </choices>
  </question>
  <question id="16">
    <wording>What is the boolean value of an empty list?</wording>
    <choices>
      <choice>True</choice>
      <choice>None</choice>
      <choice>Null</choice>
      <choice correct="yes">False</choice>
    </choices>
  </question>
  <question id="17">
    <wording>How do you import a module in Python?</wording>
    <choices>
      <choice>include</choice>
      <choice>require</choice>
      <choice>load</choice>
      <choice correct="yes">import</choice>
    </choices>
  </question>
  <question id="18">
    <wording>What is the value of 2 ** 3 in Python?</wording>
    <choices>
      <choice correct="yes">8</choice>
      <choice>6</choice>
      <choice>9</choice>
      <choice>7</choice>
    </choices>
  </question>
  <question id="19">
    <wording>How do you access the last element of a list?</wording>
    <choices>
      <choice correct="yes">list[-1]</choice>
      <choice>list[end]</choice>
      <choice>list[length]</choice>
      <choice>list.last()</choice>
    </choices>
  </question>
  <question id="20">
    <wording>What type of loop is used to iterate a fixed number of times?</wording>
    <choices>
      <choice correct="yes">for</choice>
      <choice>while</choice>
      <choice>loop</choice>
      <choice>repeat</choice>
    </choices>
  </question>
</test>
