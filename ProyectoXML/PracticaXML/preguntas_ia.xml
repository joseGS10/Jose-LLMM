    <choices>
      <choice correct="yes">Desempleo por automatización</choice>
      <choice>Mejorar la eficiencia</choice>
      <choice>Aumentar la productividad</choice>
      <choice>Reducir errores humanos</choice>
    </choices>
  </question>
