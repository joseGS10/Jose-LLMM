<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE test [
  <!ELEMENT test (question+)>
  <!ELEMENT question (wording,choices)>
  <!ELEMENT wording (#PCDATA)>
  <!ELEMENT choices (choice+)>
  <!ELEMENT choice (#PCDATA)>
  <!ATTLIST question id CDATA #REQUIRED>
  <!ATTLIST choice correct (yes|no) 'no'>
]>

<test>
  <question id="1">
    <wording>What is Artificial Intelligence (AI)?</wording>
    <choices>
      <choice>An advanced social network</choice>
      <choice>A type of computer hardware</choice>
      <choice>An interactive video game</choice>
      <choice correct="yes">A branch of computer science that simulates human intelligence</choice>
    </choices>
  </question>
  <question id="2">
    <wording>Who is considered the father of Artificial Intelligence?</wording>
    <choices>
      <choice>Alan Turing</choice>
      <choice>Marvin Minsky</choice>
      <choice>Geoffrey Hinton</choice>
      <choice correct="yes">John McCarthy</choice>
    </choices>
  </question>
  <question id="3">
    <wording>Which of the following is NOT a type of machine learning?</wording>
    <choices>
      <choice correct="yes">Passive learning</choice>
      <choice>Supervised learning</choice>
      <choice>Unsupervised learning</choice>
      <choice>Reinforcement learning</choice>
    </choices>
  </question>
  <question id="4">
    <wording>What is a language model like ChatGPT?</wording>
    <choices>
      <choice correct="yes">An AI model trained to understand and generate text</choice>
      <choice>An automatic translator</choice>
      <choice>A physical voice assistant</choice>
      <choice>A database</choice>
    </choices>
  </question>
  <question id="5">
    <wording>What is an artificial neural network?</wording>
    <choices>
      <choice>A private social network</choice>
      <choice correct="yes">A model inspired by the human brain</choice>
      <choice>A connection between computers</choice>
      <choice>A programming language</choice>
    </choices>
  </question>
  <question id="6">
    <wording>Which AI technique is based on rewards to learn?</wording>
    <choices>
      <choice>Supervised learning</choice>
      <choice correct="yes">Reinforcement learning</choice>
      <choice>Linear regression</choice>
      <choice>Decision trees</choice>
    </choices>
  </question>
  <question id="7">
    <wording>Which language is commonly used in AI projects?</wording>
    <choices>
      <choice correct="yes">Python</choice>
      <choice>HTML</choice>
      <choice>PHP</choice>
      <choice>CSS</choice>
    </choices>
  </question>
  <question id="8">
    <wording>What does 'machine learning' mean?</wording>
    <choices>
      <choice correct="yes">Automatic learning</choice>
      <choice>Machine that teaches</choice>
      <choice>Pretrained model</choice>
      <choice>Open-source language</choice>
    </choices>
  </question>
  <question id="9">
    <wording>What is an ethical concern in AI?</wording>
    <choices>
      <choice>Improving efficiency</choice>
      <choice>Increasing productivity</choice>
      <choice correct="yes">Job loss due to automation</choice>
      <choice>Reducing human error</choice>
    </choices>
  </question>
  <question id="10">
    <wording>What is deep learning?</wording>
    <choices>
      <choice>An industrial control system</choice>
      <choice>A virtual reality game</choice>
      <choice correct="yes">An AI technique based on deep neural networks</choice>
      <choice>An internet search engine</choice>
    </choices>
  </question>
  <question id="11">
    <wording>Which of the following is an AI-powered virtual assistant?</wording>
    <choices>
      <choice correct="yes">Siri</choice>
      <choice>Excel</choice>
      <choice>Paint</choice>
      <choice>Notepad</choice>
    </choices>
  </question>
  <question id="12">
    <wording>Which company developed AlphaGo?</wording>
    <choices>
      <choice correct="yes">DeepMind</choice>
      <choice>OpenAI</choice>
      <choice>Meta</choice>
      <choice>IBM</choice>
    </choices>
  </question>
  <question id="13">
    <wording>What type of AI can reason and learn like a human?</wording>
    <choices>
      <choice>Weak AI</choice>
      <choice>Supervised AI</choice>
      <choice>Reactive AI</choice>
      <choice correct="yes">General AI</choice>
    </choices>
  </question>
  <question id="14">
    <wording>What is an example of AI use in healthcare?</wording>
    <choices>
      <choice correct="yes">Medical image diagnostics</choice>
      <choice>Document printing</choice>
      <choice>Payroll management</choice>
      <choice>Furniture purchase</choice>
    </choices>
  </question>
  <question id="15">
    <wording>Which algorithm is commonly used for classification in AI?</wording>
    <choices>
      <choice correct="yes">K-nearest neighbors (KNN)</choice>
      <choice>Binary search</choice>
      <choice>Quicksort</choice>
      <choice>Merge sort</choice>
    </choices>
  </question>
  <question id="16">
    <wording>What does NLP stand for in AI context?</wording>
    <choices>
      <choice>New Programming Language</choice>
      <choice>Neuro Linguistic Programming</choice>
      <choice correct="yes">Natural Language Processing</choice>
      <choice>Primary Logic Node</choice>
    </choices>
  </question>
  <question id="17">
    <wording>What is needed to train an AI model?</wording>
    <choices>
      <choice correct="yes">Data</choice>
      <choice>A search engine</choice>
      <choice>An antivirus</choice>
      <choice>A mail server</choice>
    </choices>
  </question>
  <question id="18">
    <wording>What is a common limitation of current AI systems?</wording>
    <choices>
      <choice correct="yes">Lack of common sense</choice>
      <choice>Processing speed</choice>
      <choice>Insufficient memory</choice>
      <choice>Graphical interface</choice>
    </choices>
  </question>
  <question id="19">
    <wording>What does an AI chatbot do?</wording>
    <choices>
      <choice correct="yes">Automatically chats with users</choice>
      <choice>Searches for disk files</choice>
      <choice>Runs video games</choice>
      <choice>Encodes software</choice>
    </choices>
  </question>
  <question id="20">
    <wording>Which company created GPT-4?</wording>
    <choices>
      <choice correct="yes">OpenAI</choice>
      <choice>Google</choice>
      <choice>Apple</choice>
      <choice>Amazon</choice>
    </choices>
  </question>
</test>
